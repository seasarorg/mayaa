            value="${ '&quot;&lt;b&gt;&amp;' }" />

    <m:write id="test8" replace="false"
            value="${ '&quot;&lt;b&gt;&amp;' }" escapeXml="${ 1 == 1 }" />

    <m:write id="test9" replace="false"
            value="${ '\n\r\r\n' }" />

    <m:write id="test10" replace="false"
